import java.util.Scanner;
public class Main{
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        System.out.println("enter the array number");
        int n=sc.nextInt();
        int a[]=new int[5];
        if(n%2==0){
            System.out.println("even number");
        }else{
            System.out.println("odd number");
        }
    }
}
